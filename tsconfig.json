    "compilerOptions": {
        "module": "CommonJS",
        "target": "es2017",
        "moduleResolution": "node",
        "strict": true,
        
        "noImplicitAny": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,

        "declaration": true,
        "outDir": "./lib"
    },
	"exclude": [ "__tests__" ]
}